{
    "name": "dekmabot/laravel-novable-resource",
    "type": "library",
    "description": "The laravel resource trait to auto generate laravel nova CRUD grids",
    "keywords": [
        "laravel",
        "nova",
        "crud"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "laravel/framework": "^8.0|^9.0|^9.2",
        "laravel/nova": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "NovableResource\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        }
    ]
}
